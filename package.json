{
  "name": "playground-vue3-app",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "type-check": "vue-tsc --build",
    "lint": "eslint . --fix",
    "lint-check": "eslint . --fix-dry-run",
    "lint-all": "npm run format && npm run lint && npm run type-check",
    "format": "prettier . --write",
    "test": "vitest",
    "test-coverage": "vitest run --coverage",
    "dep-graph-clean": "rm -rf ./.dependency-graph*",
    "dep-graph-html": "npm run dep-graph-clean && npx dependency-cruiser src -I '^src' -c .dependency-cruiser.cjs -T html -f .dependency-graph.html",
    "dep-graph-help": "npx dependency-cruiser --help",
    "dep-graph-mermaid": "npm run dep-graph-clean && npm run dep-graph-mermaid-prefix && npm run dep-graph-mermaid-content && npm run dep-graph-mermaid-suffix",
    "dep-graph-mermaid-prefix": "echo '```mermaid' >> .dependency-graph.md",
    "dep-graph-mermaid-content": "npx dependency-cruiser src -I '^src' -c .dependency-cruiser.cjs -T mermaid >> .dependency-graph.md",
    "dep-graph-mermaid-suffix": "echo '```' >> .dependency-graph.md"
  },
  "dependencies": {
    "@casl/ability": "^6.7.3",
    "@casl/vue": "^2.2.2",
    "@primeuix/themes": "^1.2.4",
    "@tailwindcss/vite": "^4.1.13",
    "@vueuse/core": "^13.9.0",
    "axios": "^1.12.2",
    "material-symbols": "^0.36.1",
    "pinia": "^3.0.3",
    "pinia-plugin-persistedstate": "^4.5.0",
    "primeicons": "^7.0.0",
    "primevue": "^4.3.9",
    "tailwindcss": "^4.1.8",
    "tailwindcss-primeui": "^0.6.1",
    "vue": "^3.5.13",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@pinia/testing": "^1.0.2",
    "@primevue/auto-import-resolver": "^4.3.9",
    "@testing-library/vue": "^8.1.0",
    "@tsconfig/node22": "^22.0.2",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^24.5.2",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/eslint-plugin": "^1.3.13",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.8.1",
    "dependency-cruiser": "^17.0.1",
    "eslint": "^9.36.0",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-boundaries": "^5.0.1",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-playwright": "^2.2.2",
    "eslint-plugin-vue": "^10.5.0",
    "jiti": "^2.6.0",
    "jsdom": "^27.0.0",
    "msw": "^2.11.3",
    "npm-run-all2": "^8.0.4",
    "prettier": "^3.6.2",
    "typescript": "^5.9.2",
    "unplugin-vue-components": "^29.1.0",
    "vite": "^7.1.7",
    "vite-plugin-runtime": "^1.4.0",
    "vite-plugin-vue-devtools": "^8.0.2",
    "vitest": "^3.2.4",
    "vue-tsc": "^3.0.8"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}
