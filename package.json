{
  "name": "playground-vue3-app",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "type-check": "vue-tsc --build",
    "lint": "eslint . --fix",
    "lint-check": "eslint . --fix-dry-run",
    "lint-all": "npm run format && npm run lint && npm run type-check",
    "format": "prettier . --write",
    "test": "vitest",
    "test-coverage": "vitest run --coverage",
    "dep-graph-clean": "rm -rf ./.dependency-graph*",
    "dep-graph-html": "npm run dep-graph-clean && npx dependency-cruiser src -I '^src' -c .dependency-cruiser.cjs -T html -f .dependency-graph.html",
    "dep-graph-help": "npx dependency-cruiser --help",
    "dep-graph-mermaid": "npm run dep-graph-clean && npm run dep-graph-mermaid-prefix && npm run dep-graph-mermaid-content && npm run dep-graph-mermaid-suffix",
    "dep-graph-mermaid-prefix": "echo '```mermaid' >> .dependency-graph.md",
    "dep-graph-mermaid-content": "npx dependency-cruiser src -I '^src' -c .dependency-cruiser.cjs -T mermaid >> .dependency-graph.md",
    "dep-graph-mermaid-suffix": "echo '```' >> .dependency-graph.md"
  },
  "dependencies": {
    "@casl/ability": "^6.7.3",
    "@casl/vue": "^2.2.2",
    "@fullcalendar/core": "^6.1.19",
    "@fullcalendar/daygrid": "^6.1.19",
    "@fullcalendar/interaction": "^6.1.19",
    "@fullcalendar/vue3": "^6.1.19",
    "@primeuix/themes": "^1.2.5",
    "@primevue/forms": "^4.4.1",
    "@tailwindcss/vite": "^4.1.17",
    "@tanstack/vue-query": "^5.91.2",
    "@vueuse/core": "^14.0.0",
    "axios": "^1.13.2",
    "luxon": "^3.7.2",
    "material-symbols": "^0.40.0",
    "pinia": "^3.0.4",
    "pinia-plugin-persistedstate": "^4.7.1",
    "primeicons": "^7.0.0",
    "primevue": "^4.4.1",
    "tailwindcss": "^4.1.8",
    "tailwindcss-primeui": "^0.6.1",
    "vee-validate": "^5.0.0-beta.0",
    "vue": "^3.5.13",
    "vue-router": "^4.6.3",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@faker-js/faker": "^10.1.0",
    "@pinia/testing": "^1.0.3",
    "@primevue/auto-import-resolver": "^4.4.1",
    "@testing-library/user-event": "^14.6.1",
    "@testing-library/vue": "^8.1.0",
    "@tsconfig/node22": "^22.0.5",
    "@types/jsdom": "^21.1.7",
    "@types/luxon": "^3.7.1",
    "@types/node": "^24.10.1",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "@vitejs/plugin-vue": "^6.0.2",
    "@vitest/coverage-v8": "^4.0.13",
    "@vitest/eslint-plugin": "^1.4.3",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.8.1",
    "dependency-cruiser": "^17.3.1",
    "eslint": "^9.39.1",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-boundaries": "^5.2.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-playwright": "^2.3.0",
    "eslint-plugin-vue": "^10.5.1",
    "flush-promises": "^1.0.2",
    "jiti": "^2.6.1",
    "jsdom": "^27.2.0",
    "msw": "^2.12.2",
    "npm-run-all2": "^8.0.4",
    "prettier": "^3.6.2",
    "typescript": "^5.9.3",
    "unplugin-vue-components": "^30.0.0",
    "vite": "^7.2.4",
    "vite-plugin-runtime": "^1.4.0",
    "vite-plugin-vue-devtools": "^8.0.5",
    "vitest": "^4.0.7",
    "vue-tsc": "^3.1.4",
    "wait-for-expect": "^4.0.0"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}
